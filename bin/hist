#!/usr/bin/env zsh

if [[ `hostname` == "jxck.io" ]]; then
  histuniq
  echo "histuniq"
elif [[ `hostname` =~ ^.*\.com$ ]]; then
  histuniq
  echo "histuniq"
else
  histuniq
  echo "histuniq"

  cat ~/.zsh_history | ssh jxck.io "cat >> ~/.zsh_history"
  echo "push to remote"

  ssh jxck.io "cat ~/.zsh_history" >> ~/.zsh_history
  echo "pull from remote"

  histuniq
  echo "histuniq"
fi
