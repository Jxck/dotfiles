alias gith="cat <<EOF
[push branch] $ git push origin dev
[ck branch]   $ git checkout -b dev origin/dev
[del branch]  $ git push origin :dev
[sync branch] $ git fetch --prune
EOF"

alias empty="sudo cp /dev/null $1"
alias re="exec $SHELL"
alias cdgo="cd $GOPATH"
alias timeset="sudo date -s \"$(curl -s --head http://www.google.co.jp | grep ^Date | cut -b 7-)\""
